<p id="notice"><%= notice %></p>

<h1>Gpslocations</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Address</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @gpslocations.each do |gpslocation| %>
      <tr>
        <td><%= gpslocation.name %></td>
        <td><%= gpslocation.address %></td>
        <td><%= gpslocation.latitude %></td>
        <td><%= gpslocation.longitude %></td>
        <td><%= link_to 'Show', gpslocation %></td>
        <td><%= link_to 'Edit', edit_gpslocation_path(gpslocation) %></td>
        <td><%= link_to 'Destroy', gpslocation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Gpslocation', new_gpslocation_path %>
